  "scripts": {
    "clean": "rimraf dist node_modules",
    "build": "npm-run-all --parallel build:*",
    "build:ts": "tsc",
    "build:next": "next build",
    "build:static": "copyfiles  \"./static/*\" dist/client/",
    "ci:heroku:build": "docker build --rm=false -t registry.heroku.com/$npm_package_name/web .",
    "ci:heroku:login": "docker login -u $HEROKU_LOGIN -p $HEROKU_TOKEN registry.heroku.com",
    "ci:heroku:push": "docker push registry.heroku.com/$npm_package_name/web:latest",
    "ci:heroku:release": "chmod +x ./scripts/release.sh && ./scripts/release.sh $npm_package_name",
    "ci:surge": "surge -p ./dist/client -d $npm_package_name.surge.sh",
    "deps:all": "npm ci",
    "deps:production": "npm ci --production",
    "docker:build": "cross-env docker build -t $npm_package_name .",
    "docker:run": "cross-env docker run -p 8080:80 $npm_package_name",
    "heroku:login": "heroku container:login",
    "heroku:push": "heroku container:push web --app $npm_package_name",
    "heroku:release": "heroku container:release web --app $npm_package_name",
    "heroku:open": "heroku open",
    "heroku": "npm-run-all heroku:*",
    "heroku-init": "heroku login && heroku create $npm_package_name",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint --fix server/**/*.css",
    "lint:deps": "npm audit",
    "lint:es": "eslint --fix --ext=.ts,.tsx,.js server/ config/ pages/ components/",
    "lint:ts": "tslint --project .",
    "start": "cross-env NODE_PATH=dist/server node dist/server",
    "surge": "surge -p ./dist/client -d $npm_package_name.surge.sh"
  }
}
